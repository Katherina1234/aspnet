@model lab1_1.Models.Timetable
@{
    ViewBag.Title = "Видалення поїзда з розкладу";
}
<h2>@ViewBag.Title</h2>
<div>
    <div >
        <h3>Номер поїзда</h3>
        <h4>
            @Html.DisplayFor(model => model.train_num)
        </h4>
        <h3>Прибуття</h3>
        <h4>
            @Html.DisplayFor(model => model.prub)
        </h4>
        <h3>Номер колії</h3>
        <h4>
            @Html.DisplayFor(model => model.num_koli)
        </h4>
    </div>
    <div>
        <form asp-controller="Home" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Видалити" />
            </div>
        </form>
    </div>
</div>